    key <AC09>	{[	 l,	 L		]};
    key <AD11> {[apostrophe,quotedbl]};
    key <AD12> {[minus, underscore]};

    key <AC11> {[equal,ampersand]};


    key <AB01>	{[	 z,	 Z		]};
